{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "linebreak-style": 0,
      "react/jsx-filename-extension":         [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-props-no-spreading": [0, {
        "html": "ignore",
        "custom": "ignore",
        "explicitSpread": "ignore"
    }]
    }
}
